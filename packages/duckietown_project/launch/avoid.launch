<?xml version="1.0" encoding="utf-8"?>
<launch>

    <remap from="avoiders_controller_node/left_wheel_encoder_node/tick" to="left_wheel_encoder_node/tick"/>
    <remap from="avoiders_controller_node/right_wheel_encoder_node/tick" to="right_wheel_encoder_node/tick"/>
    <remap from="avoiders_controller_node/avoidance_path" to="detect_obstacle/avoidance_path"/>
    <remap from="detect_obstacle/avoidance_done" to="avoiders_controller_node/avoidance_done"/>
    <remap from="detect_obstacle/lane_pose" to="lane_filter_node/lane_pose"/>

    <include file="$(find avoiders)/launch/avoiders.launch">
    <arg name="veh" value="$(env VEHICLE_NAME)"/>
    </include>





</launch>